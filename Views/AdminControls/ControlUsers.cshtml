@model EditUserViewModel


@{
    Layout = "_AdminLayout";
}

<form asp-action="Edit" asp-controller="Users">
    <div asp-validation-summary="All" class="text-danger"></div>
    <div class="form-group">
        <input type="hidden" asp-for="Id" />
    </div>
    <div class="form-group">
        <label asp-for="UserEmail" class="control-label">Email</label>
        <input type="text" asp-for="UserEmail" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="UserName" class="control-label">Год рождения</label>
        <input type="number" asp-for="UserName" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Сохранить"  />
    </div>
</form>



    <table id="userTable">
        <thead>
        <tr>
            <th>Email</th>
            <th>UserName</th>
            <th>UserSurname</th>
            <th>UserPhone</th>
            <th>Delete|Update</th>
			
        </tr>
        </thead>
    </table>

     <link href="//cdn.datatables.net/1.11.3/css/jquery.dataTables.min.css" rel="stylesheet"/>
@section scripts{
 
  <script src="/js/"> </script>
  <script>tableTeam()</script>
  
}
        











    
    